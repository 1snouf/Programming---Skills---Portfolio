# 07-SomeCounting/main.py

print("1) Count up 0 to 50 (step 1)")
for i in range(0, 51, 1):
    print(i)

print("\n2) Count down 50 to 0 (step -1)")
for i in range(50, -1, -1):
    print(i)

print("\n3) Count up 30 to 50 (step 1)")
for i in range(30, 51, 1):
    print(i)

print("\n4) Count down 50 to 10 (step -2)")
for i in range(50, 9, -2):
    print(i)

print("\n5) Count up 100 to 200 (step 5)")
for i in range(100, 201, 5):
    print(i)
